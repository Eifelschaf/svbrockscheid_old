        <item name="android:background">@drawable/svtheme_edit_text_holo_light</item>
    </style>

    <style name="ButtonSVTheme" parent="android:Widget.Holo.Light.Button">
        <item name="android:background">@drawable/svtheme_btn_default_holo_light</item>
